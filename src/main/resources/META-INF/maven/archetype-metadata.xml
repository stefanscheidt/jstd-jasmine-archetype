<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="helloworld-testdriven-mvn"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileSets>

        <fileSet encoding="UTF-8">
            <directory></directory>
            <includes>
                <include>pom.xml</include>
                <include>*.conf</include>
                <include>*.sh</include>
            </includes>
        </fileSet>

        <fileSet encoding="UTF-8">
            <directory>src/main/webapp</directory>
            <includes>
                <include>**/*.js</include>
                <include>**/*.html</include>
                <include>**/*.xml</include>
            </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>src/test/webapp</directory>
            <includes>
                <include>**/*.html</include>
            </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>src/test/webapp</directory>
            <includes>
                <include>lib/**/*.*</include>
                <include>unit/**/*.*</include>
                <include>*.html</include>
            </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/test/webapp</directory>
            <includes>
                <include>ui/**/*.*</include>
            </includes>
        </fileSet>
    </fileSets>
</archetype-descriptor>
